<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="@dimen/horizontalGap"
    android:keyHeight="@dimen/key_height"
    android:keyWidth="9%p"
    android:verticalGap="@dimen/verticalGap">

    <Row>
        <Key
            android:codes="113"
            android:keyEdgeFlags="left"
            android:keyLabel="q" />
        <Key
            android:codes="119"
            android:keyLabel="w" />
        <Key
            android:codes="101"
            android:keyLabel="e"
            android:popupCharacters="&#x00E9;&#x00E8;&#x00EA;&#x00EB;&#x0117;"
            android:popupKeyboard="@xml/kbd_popup_template"/>
        <Key
            android:codes="114"
            android:keyLabel="r" />
        <Key
            android:codes="116"
            android:keyLabel="t" />
        <Key
            android:codes="122"
            android:keyLabel="z" />
        <Key
            android:codes="117"
            android:keyLabel="u"
            android:popupCharacters="&#x00FB;&#x00F9;&#x00FA;&#x016B;" />
        <Key
            android:codes="105"
            android:keyLabel="i" />
        <Key
            android:codes="111"
            android:keyLabel="o"
            android:popupCharacters="&#x00F4;&#x00F2;&#x00F3;&#x00F5;&#x0153;&#x00F8;&#x014D;"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="112"
            android:keyLabel="p" />
        <Key
            android:codes="252"
            android:keyEdgeFlags="right"
            android:keyLabel="ü" />
    </Row>

    <Row>
        <Key
            android:codes="97"
            android:keyEdgeFlags="left"
            android:keyLabel="a"
            android:popupCharacters="&#x00E2;&#x00E0;&#x00E1;&#x00E6;&#x00E3;&#x00E5;&#x0101;"
            android:popupKeyboard="@xml/kbd_popup_template"/>
        <Key
            android:codes="115"
            android:keyLabel="s"
            android:popupCharacters="&#x00DF;&#x015B;&#x0161;"
            android:popupKeyboard="@xml/kbd_popup_template"/>
        <Key
            android:codes="100"
            android:keyLabel="d" />
        <Key
            android:codes="102"
            android:keyLabel="f" />
        <Key
            android:codes="103"
            android:keyLabel="g" />
        <Key
            android:codes="104"
            android:keyLabel="h" />
        <Key
            android:codes="106"
            android:keyLabel="j" />
        <Key
            android:codes="107"
            android:keyLabel="k" />
        <Key
            android:codes="108"
            android:keyLabel="l" />
        <Key
            android:codes="246"
            android:keyLabel="ö" />
        <Key
            android:codes="228"
            android:keyEdgeFlags="right"
            android:keyLabel="ä" />
    </Row>

    <Row android:keyWidth="10%p">
        <Key
            android:codes="@integer/key_delete"
            android:isRepeatable="true"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/ic_backspace_black_24dp"
            android:keyWidth="15%p" />
        <Key
            android:codes="121"
            android:keyLabel="y" />
        <Key
            android:codes="120"
            android:keyLabel="x" />
        <Key
            android:codes="99"
            android:keyLabel="c" />
        <Key
            android:codes="118"
            android:keyLabel="v" />
        <Key
            android:codes="98"
            android:keyLabel="b" />
        <Key
            android:codes="110"
            android:keyLabel="n"
            android:popupCharacters="&#x00F1;&#x0144;"
            android:popupKeyboard="@xml/kbd_popup_template"/>
        <Key
            android:codes="109"
            android:keyLabel="m" />
        <Key
            android:codes="@integer/key_shift"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/small_pressed_t"
            android:keyWidth="15%p" />
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key
            android:codes="@integer/key_return"
            android:keyEdgeFlags="left"
            android:keyWidth="15%p" />
        <Key
            android:codes="44"
            android:keyLabel=","
            android:keyWidth="12%p" />
        <Key
            android:codes="32"
            android:isRepeatable="true"
            android:keyLabel="@string/space"
            android:keyWidth="31%p" />
        <Key
            android:codes="@integer/key_language"
            android:keyIcon="@drawable/ic_g_translate_black_24dp"
            android:keyWidth="15%p" />
        <Key
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="12%p" />
        <Key
            android:codes="@integer/key_symbol"
            android:keyEdgeFlags="right"
            android:keyLabel="@string/numbers"
            android:keyWidth="15%p" />
    </Row>
</Keyboard>